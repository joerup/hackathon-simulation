{
  "name": "linkedout-block-arena",
  "productName": "LinkedOut - BlockArena",
  "version": "1.0.0",
  "type": "module",
  "description": "LinkedOut - BlockArena: Hackathon simulation game",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:mac": "npx @electron/packager . LinkedOut --platform=darwin --arch=arm64 --out=dist --overwrite --extra-resource=sample-resumes",
    "build:mac-intel": "npx @electron/packager . LinkedOut --platform=darwin --arch=x64 --out=dist --overwrite --extra-resource=sample-resumes",
    "build:win": "npx @electron/packager . LinkedOut --platform=win32 --arch=arm64 --out=dist --overwrite --extra-resource=sample-resumes",
    "build:win-intel": "npx @electron/packager . LinkedOut --platform=win32 --arch=x64 --out=dist --overwrite --extra-resource=sample-resumes",
    "build:linux": "npx @electron/packager . LinkedOut --platform=linux --arch=x64 --out=dist --overwrite --extra-resource=sample-resumes",
    "build:all": "npm run build:mac && npm run build:mac-intel && npm run build:win && npm run build:win-intel && npm run build:linux",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "@electron/packager": "^18.4.4",
    "electron": "^38.1.2"
  },
  "dependencies": {
    "three": "^0.180.0"
  }
}
